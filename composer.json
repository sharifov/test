		"yiisoft/yii2-debug" : "~2.1",
		"yiisoft/yii2-gii" : "~2.2",
		"yiisoft/yii2-faker" : "~2.0",
		"eaglewu/swoole-ide-helper": "dev-master",
		"codeception/codeception": "^4.1",
		"codeception/module-asserts": "^1.3",
		"codeception/module-yii2": "^1.1",
		"codeception/module-filesystem": "^1.0",
		"codeception/verify": "2.2.0",
		"symfony/browser-kit": "5.4.3",
		"codeception/phpunit-wrapper": "9.0.9",
		"phpunit/phpunit": "9.5.20",
		"vimeo/psalm": "^3.18",
		"phpstan/phpstan": "^0.12.76",
		"friendsofphp/php-cs-fixer": ">=2.19.0",
		"proget-hq/phpstan-yii2": "^0.7.3",
		"squizlabs/php_codesniffer": "^3.5",
		"overtrue/phplint": "^2.3",
		"brainmaestro/composer-git-hooks": "^2.8",
		"codeception/module-webdriver": "^1.2",
		"codeception/module-rest": "^1.2",
		"codeception/module-phpbrowser": "^1.0",
		"fakerphp/faker": "^1.14",
		"redcomponent/teststatistics": "dev-master",
		"phpcompatibility/php-compatibility": "dev-develop"
	},
	"autoload-dev": {
		"psr-2": {
			"app\\": "./"
		},
		"classmap": [
			"common/models/",
			"frontend/models/",
			"common/components/",
			"vendor/yiisoft/yii2/Yii.php",
			"src/"
		],
		"exclude-from-classmap": [
			"docker/",
			"vagrant/",
			"node_modules/"
		]
	},
	"config" : {
		"process-timeout" : 1800,
		"fxp-asset" : {
			"enabled" : false
		},
		"optimize-autoloader": true,
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"dealerdirect/phpcodesniffer-composer-installer": false
		}
	},
	"repositories" : [
		{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-rbac-export-import.git"
		},
		{
			"type": "git",
			"url": "git@bitbucket.org:techork/yii2-bootstrap4-glyphicons.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-mattermost-log-target.git"
		},

		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-translations-export-import-csv.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-virtual-cron.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-prometheus-client.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-feature-flag.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:techork/yii2-search-module.git"
		}
	],
	"scripts": {
		"cs-fix": [
			"vendor/bin/php-cs-fixer fix --config=.php_cs --verbose --show-progress=dots"
		],
		"cs-check": "phpcs",
		"cs-fix2": "phpcbf",
		"assets-compressor": [
			"rm -rf frontend/web/all_shared/build/*",
			"rm -rf frontend/web/client_chat/build/*",
			"rm -rf frontend/web/web_phone/build/*",
			"php yii asset frontend/config/assets-config.php frontend/config/assets-prod.php"
		],
		"lint": "vendor/bin/phplint",
		"cghooks": "vendor/bin/cghooks",
		"post-install-cmd": [
			"cghooks add --ignore-lock",
			"cghooks update"
		],
		"post-update-cmd": [
			"cghooks update"
		],
        "apidoc-gen": "node_modules/apidoc/bin/apidoc -c ./apidoc.json -i \"./webapi/modules/\" -i \"./webapi/controllers/\" -o \"./webapi/web/doc\" -f \".*\\\\.php$\"",
        "phpdoc": "phpDocumentor",
		"php-compatibility": "phpcs -p ./frontend --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --report-full=report.txt --extensions=php --runtime-set testVersion 8.0"
	},
	"extra": {
		"hooks": {
			"pre-commit": [
				"if [ -e \"docker-pre-commit-new.sh\" ]; then\n    docker-compose -f .docker/docker-compose.yaml run --rm console bash -c \"./docker-pre-commit-new.sh\"\nelif [ -e \"docker-pre-commit.sh\" ]; then\n docker-compose -f docker/local/docker-compose.yaml run --rm console bash -c \"./docker-pre-commit.sh\"\nelse\n bash pre-commit.sh\nfi"
			]
		}
	}
}
