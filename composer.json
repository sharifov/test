{
	"name" : "yiisoft/yii2-app-advanced",
	"description" : "Sales",
	"keywords" : [
		"yii2",
		"framework",
		"sales"
	],
	"homepage" : "http://www.yiiframework.com/",
	"type" : "project",
	"license" : "BSD-3-Clause",
	"support" : {
		"issues" : "https://github.com/yiisoft/yii2/issues?state=open",
		"forum" : "http://www.yiiframework.com/forum/",
		"wiki" : "http://www.yiiframework.com/wiki/",
		"irc" : "irc://irc.freenode.net/yii",
		"source" : "https://github.com/yiisoft/yii2"
	},
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"require" : {
		"php" : ">=7.4",
		"yiisoft/yii2" : "~2.0.24",
		"yiisoft/yii2-bootstrap" : "^2.0.10",
		"yiisoft/yii2-swiftmailer" : "^2.1",
		"yiisoft/yii2-httpclient" : "^2.0.11",
		"2amigos/yii2-date-picker-widget" : "~1.0",
		"andrewblake1/yii2-credit-card" : "dev-master",
		"kartik-v/yii2-widget-select2" : "2.1.7",
		"authorizenet/authorizenet" : "~1.9.9",
		"2amigos/yii2-ckeditor-widget" : "^2.1",
		"apimatic/jsonmapper" : "~1.3",
		"yii2tech/csv-grid" : "^1.0@dev",
		"borales/yii2-phone-input" : "0.3.0",
		"keltstr/yii2-simplehtmldom" : "dev-master",
		"2amigos/yii2-date-time-picker-widget" : "~1.0",
		"yiisoft/yii2-jui" : "^2.0",
		"ext-json" : "*",
		"ext-curl" : "*",
		"ext-gd" : "*",
		"ext-zip" : "*",
		"ext-pdo" : "*",
		"ext-geoip" : "*",
		"ext-intl" : "*",
		"lajax/yii2-translate-manager" : "^1.7",
		"webvimark/module-user-management" : "^1.0",
		"kartik-v/yii2-export" : ">=1.3",
		"kartik-v/yii2-grid" : "dev-master",
		"kartik-v/yii2-editable" : "dev-master",
		"kartik-v/yii2-date-range" : "*",
		"kartik-v/yii2-krajee-base" : "*",
		"kartik-v/yii2-widget-timepicker" : "dev-master",
		"vova07/yii2-imperavi-widget" : "*",
		"kdn/yii2-json-editor" : "*",
		"workerman/workerman" : "^3.5",
		"yiisoft/yii2-queue": "^2.3",
		"pda/pheanstalk": "^3.2",
		"index0h/yii2-log": "0.0.3",
		"twilio/sdk": "^5.34",
		"2amigos/qrcode-library": "^2.0",
		"aki/yii2-bot-telegram": "*",
		"yii2mod/yii2-rbac": "*",
		"unclead/yii2-multiple-input": "~2.0",
		"kartik-v/yii2-widget-activeform": "dev-master",
		"kartik-v/yii2-widget-datepicker": "@dev",
		"yiisoft/yii2-bootstrap4": "^2.0",
		"kartik-v/yii2-bootstrap4-dropdown": "@dev",
		"rmrevin/yii2-fontawesome": "3.4.0",
		"ext-openssl": "*",
		"soundasleep/html2text": "~1.1",
		"ext-zlib": "*",
		"webmozart/assert": "^1.7",
		"thamtech/yii2-uuid": "^1.2",
		"kivork/yii2-rbac-export-import": "@dev",
		"2amigos/2fa-library": "^2.0",
		"yiisoft/yii2-redis": "~2.0.0",
		"kivork/yii2-bootstrap4-glyphicons": "*",
		"2amigos/yii2-multi-select-widget": "*",
		"kivork/yii2-mattermost-log-target": "@dev",
		"kivork/yii2-virtual-cron": "@dev",
		"kivork/yii2-prometheus-client": "@dev",
		"sorokinmedia/yii2-centrifugo": "*",
		"stefangabos/world_countries": "^2.1",
		"league/flysystem": "^2.0",
		"league/flysystem-aws-s3-v3": "^2.0"
	},
	"require-dev" : {
		"yiisoft/yii2-debug" : "~2.1",
		"yiisoft/yii2-gii" : "~2.1",
		"yiisoft/yii2-faker" : "~2.0",
		"eaglewu/swoole-ide-helper": "dev-master",
		"codeception/codeception": "^4.0",
		"codeception/module-asserts": "^1.0",
		"codeception/module-yii2": "^1.0",
		"codeception/module-filesystem": "^1.0",
		"codeception/verify": "~0.5.0 || ~1.1.0",
		"symfony/browser-kit": ">=2.7 <=4.2.4",
		"codeception/phpunit-wrapper": "^7.8",
		"phpunit/phpunit": "^7.0",
		"vimeo/psalm": "^3.13",
		"phpstan/phpstan": "^0.12.37",
		"friendsofphp/php-cs-fixer": "^2.16",
		"proget-hq/phpstan-yii2": "^0.7.3",
		"squizlabs/php_codesniffer": "^3.5",
		"overtrue/phplint": "^2.3",
		"brainmaestro/composer-git-hooks": "^2.8"
	},
	"autoload-dev": {
		"psr-2": {
			"app\\": "./"
		},
		"classmap": [
			"common/models/",
			"frontend/models/",
			"common/components/",
			"vendor/yiisoft/yii2/Yii.php",
			"sales/"
		],
		"exclude-from-classmap": [
			"docker/",
			"vagrant/",
			"node_modules/"
		]
	},
	"config" : {
		"process-timeout" : 1800,
		"fxp-asset" : {
			"enabled" : false
		},
		"optimize-autoloader": true
	},
	"repositories" : [
		{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		},
		{
			"type": "vcs",
			"url": "https://stash.travelinsides.com/scm/lib/yii2-rbac-export-import.git"
		},
		{
			"type": "git",
			"url": "https://stash.travelinsides.com/scm/lib/yii2-bootstrap4-glyphicons.git"
		},
		{
			"type": "vcs",
			"url": "https://stash.travelinsides.com/scm/lib/yii2-mattermost-log-target.git"
		},
		{
			"type": "vcs",
			"url": "https://stash.travelinsides.com/scm/lib/yii2-virtual-cron.git"
		},
		{
			"type": "vcs",
			"url": "https://stash.travelinsides.com/scm/lib/yii2-prometheus-client.git"
		}
	],
	"scripts": {
		"cs-fix": [
			"vendor/bin/php-cs-fixer fix --config=.php_cs --verbose --show-progress=dots"
		],
		"cs-check": "phpcs",
		"cs-fix2": "phpcbf",
		"assets-compressor": [
			"rm -rf frontend/web/all_shared/build/*",
			"rm -rf frontend/web/fontawesome/build/*",
			"rm -rf frontend/web/client_chat/build/*",
			"rm -rf frontend/web/web_phone/build/*",
			"php yii asset frontend/config/assets-config.php frontend/config/assets-prod.php"
		],
		"lint": "vendor/bin/phplint",
		"cghooks": "vendor/bin/cghooks",
		"post-install-cmd": [
			"cghooks add --ignore-lock",
			"cghooks update"
		],
		"post-update-cmd": [
			"cghooks update"
		]
	},
	"extra": {
		"hooks": {
			"pre-commit": [
				"bash pre-commit.sh"
			]
		}
	}
}
